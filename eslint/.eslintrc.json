{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      "off",
      "always"
    ],
    "react/no-array-index-key": [
      "off",
      "always"
    ],
    "react/jsx-tag-spacing": [
      "off",
      "always"
    ],
    "no-underscore-dangle": [
      "off",
      "always"
    ],
    "no-console": "off",
    "no-alert": "off"
  }
}
